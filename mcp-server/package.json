{
  "name": "chatbrowse-mcp-server",
  "version": "1.0.0",
  "description": "MCP Server with Playwright for ChatBrowse",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "node ./scripts/build.js",
    "start": "node dist/index.js",
    "postbuild": "node ./scripts/post-build.js",
    "install-server": "node dist/install.js",
    "dev": "tsc-watch --onSuccess \"node dist/index.js\""
  },
  "bin": {
    "chatbrowse-mcp": "./dist/index.js"
  },
  "keywords": [
    "mcp",
    "chatbrowse",
    "playwright",
    "web-automation"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "playwright-chromium": "^1.40.0"
  },
  "devDependencies": {
    "@types/chrome": "^0.0.254",
    "@types/node": "^20.10.0",
    "typescript": "^5.3.2",
    "tsc-watch": "^6.0.4"
  }
} 